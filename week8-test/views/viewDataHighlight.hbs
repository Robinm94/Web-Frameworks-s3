<table border="1">
  <tr>
    <th>Student Id</th>
    <th>Name</th>
    <th>Program</th>
    <th>Gpa</th>
  </tr>
  {{#each data}}
    <tr {{#highlight this}} style="background-color: yellow;" {{/highlight}}>
      <td>{{this.studId}}</td>
      <td>{{this.name}}</td>
      <td>{{this.program}}</td>
      <td>{{this.gpa}}</td>
    </tr>
  {{/each}}
</table>